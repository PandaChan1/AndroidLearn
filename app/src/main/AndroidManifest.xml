<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfirstapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RecylerView.GridRvActivity"></activity>
        <activity android:name=".RecylerView.HorizonRvActivity" />
        <activity android:name=".RecylerView.LinearActivity" />
        <activity android:name=".RecylerView.recyclerViewActivity" />
        <activity android:name=".Imageview_Activity" />
        <activity android:name=".TextActivity" />
        <activity android:name=".EditActivity" />
        <activity android:name=".CheckBoxActivity" />
        <activity android:name=".RadioActivity" />
        <activity android:name=".Button_Activity" />
        <activity android:name=".textviewActivity" />
        <activity android:name=".Listview.ListviewActivity" />
        <activity android:name=".Gridview.GridviewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>